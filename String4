let str = "hello world";

 let count=0;
  for (let i=0; i<str.length; i++)
  {
     if(str[i]!="A"&&str[i]!="E"&&str[i]!="I"&&str[i]!="O"&&str[i]!="U" && str[i]!="a" &&str[i]!="e" &&str[i]!="i" &&str[i]!="o" && str[i]!="u" && str[i]!=" ")
     {
        count++
     }
  }
   console.log (`Total number of consonants are ${count}`)
